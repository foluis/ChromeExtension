{
    "manifest_version": 2,
    "name": "Forero Extension 2",
    "description": "This is a prove of concept chrome extension",
    "version": "0.1"

    ,"content_scripts": [
      {
        "matches": ["https://foluis.github.io/ClientForm/Payment.html"],
        "css": ["deleteStyles.css"],
        "js": ["init.js"]
      }
    ]

    ,"background": {
      "scripts": [        
